{
    "window.zoomLevel": 0,
    "editor.fontSize": 14,
    "editor.fontWeight": "300",
    "workbench.fontAliasing": "antialiased",
    "files.autoSave": "onWindowChange",
    "editor.tabSize": 2,
    "terminal.external.osxExec": "iTerm.app",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/tmp": true,
        "**/.tmp": true
    },
    "editor.renderWhitespace": "all",
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": "seti",
    "html.suggest.ionic": false,
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "extensions.autoUpdate": true,
    "editor.codeLens": true,
    "editor.minimap.enabled": false,
    "editor.autoIndent": true,
    "explorer.autoReveal": false,
    "workbench.editor.enablePreview": false,
    "explorer.openEditors.visible": 0,
    "terminal.explorerKind": "external",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "git.enableSmartCommit": true,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.gitExplorer.enabled": false,
    "gitlens.resultsExplorer.files.compact": false,
    "gitlens.explorers.avatars": false,
    "gitlens.resultsExplorer.files.layout": "tree",
    "gitlens.statusBar.enabled": false,
    "workbench.statusBar.feedback.visible": false,
    "gitlens.keymap": "alternate",
    "explorer.confirmDelete": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "terminal.integrated.rendererType": "dom",
    "codebucket.issueTrackers": [
        {
            "type": "jira",
            "host": "https://hcpeer2peer.atlassian.net",
            "projectKeys": ["PPL"]
        }
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
}